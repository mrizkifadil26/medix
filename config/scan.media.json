{
  "concurrency": 8,
  "scan": [
    {
      "name": "movies.todo",
      "type": "movies",
      "phase": "raw",
      "include": [
        {
          "label": "foreign",
          "path": "/mnt/e/TODO/Movies/Foreign"
        },
        {
          "label": "unfoldered",
          "path": "/mnt/e/TODO/Movies/Unfoldered"
        },
        {
          "label": "by-year",
          "path": "/mnt/e/TODO/Movies/By Year"
        },
        {
          "label": "fresh",
          "path": "/mnt/c/Users/Rizki/Downloads/Media/Movies"
        }
      ],
      "exclude": [],
      "output": "data/media/movies.raw.json",
      "options": {
        "exts": [
          ".mkv",
          ".mp4"
        ]
      }
    },
    {
      "name": "movies.staged",
      "type": "movies",
      "phase": "staged",
      "include": [
        {
          "label": "foldered",
          "path": "/mnt/e/TODO/Movies/Foldered"
        }
      ],
      "exclude": [],
      "output": "data/media/movies.staged.json"
    },
    {
      "name": "movies.media",
      "type": "movies",
      "phase": "media",
      "include": [
        {
          "label": "movies",
          "path": "/mnt/e/Media/Movies"
        }
      ],
      "exclude": [],
      "output": "data/media/movies.media.json"
    },
    {
      "name": "tv",
      "type": "tv",
      "phase": "media",
      "include": [
        {
          "label": "tv",
          "path": "/mnt/e/Media/TV Shows"
        },
        {
          "label": "external",
          "path": "/mnt/d/TV Shows"
        },
        {
          "label": "fresh",
          "path": "/mnt/c/Users/Rizki/Downloads/Media/TV Shows"
        }
      ],
      "exclude": [],
      "output": "data/media/tv.media.json"
    }
  ]
}
