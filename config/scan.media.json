{
  "concurrency": 8,
  "scan": [
    {
      "name": "movies",
      "type": "media",
      "include": [
        {
          "label": "movies",
          "path": "/mnt/e/Media/Movies"
        },
        {
          "label": "foreign",
          "path": "/mnt/e/TODO/Movies/Foreign"
        },
        {
          "label": "foldered",
          "path": "/mnt/e/TODO/Movies/Foldered"
        },
        {
          "label": "unfoldered",
          "path": "/mnt/e/TODO/Movies/Unfoldered"
        },
        {
          "label": "by-year",
          "path": "/mnt/e/TODO/Movies/By Year"
        }
      ],
      "exclude": [],
      "final_root": "/mnt/e/Media/Movies",
      "output": "media/movies.raw.json"
    },
    {
      "name": "tv",
      "type": "media",
      "include": [
        {
          "label": "tv",
          "path": "/mnt/e/Media/TV Shows"
        },
        {
          "label": "external",
          "path": "/mnt/d/TV Shows"
        }
      ],
      "exclude": [],
      "final_root": "/mnt/e/Media/TV Shows",
      "output": "media/tv.json"
    }
  ]
}
